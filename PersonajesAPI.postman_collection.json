									"pm.environment.set(\"status\", jsonData.status);\r",
									"pm.environment.set(\"species\", jsonData.species);\r",
									"pm.environment.set(\"type\", jsonData.type);\r",
									"pm.environment.set(\"gender\", jsonData.gender);\r",
									"\r",
									"\r",
