									"\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"pm.test(\"Validar Nombre de Personaje\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.name).to.eql(pm.environment.get(\"expectedName\"));\r",
									"\r",
									"    \r",
									"});\r",
									"/*\r",
									"pm.test(\"Validar Origen de Personaje\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.origin.name).to.eql(\"Earth (C-137)\");\r",
									"});\r",
									"**/\r",
									"pm.test(\"Response time is less than 1000ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									"\r",
									"/*\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.sendRequest('https://rickandmortyapi.com/api/character/2', \r",
									"    \r",
									"    function (err, res) {\r",
									"        \r",
									"     \r",
									"        pm.expect(err).to.not.be.ok;\r",
									"        pm.expect(res).to.have.property('code', 200);\r",
									"        pm.expect(res).to.have.property('status', 'OK');\r",
									"      \r",
									"        pm.expect(res.text()).to.include(\"Morty Smith\");\r",
